    "@types/npmcli__package-json": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vercel/ncc": "^0.36.1",
    "bun-types": "^0.6.14",
    "eslint": "^8.44.0",
